package colecoes;

import java.util.ArrayDeque;
import java.util.Deque;

public class Pilha {

	public static void main(String[] args) {
		Deque<String> livros = new ArrayDeque<>();
		
		livros.add("a");
		livros.push("b");
		livros.push("c");
		
		System.out.println(livros.peek());
		System.out.println(livros.element());
		
		

	}

}
