package arrays;

import java.util.Arrays;

public class Exercicio {

	public static void main(String[] args) {
		double [] notasAlunoA = new double[3];
		notasAlunoA[0] = 6.6;
		notasAlunoA[1] = 7.0;
		notasAlunoA[2] = 9.2;
		
		System.out.println(Arrays.toString(notasAlunoA));
		
		double total = 0;
		for(int i = 0;i<3;i++) {
			total+= notasAlunoA[i];
		}
		System.out.println(total/3);
				
		

	}

}
