package oo.heranca;

public class Jogo {

	public static void main(String[] args) {
		Monstro mostro = new Monstro();
		mostro.x = 10;
		mostro.y = 10;
		
		Heroi heroi = new Heroi();
		heroi.x = 10;
		heroi.y = 11;
		
		System.out.println(mostro.vida);
		System.out.println(heroi.vida);
		
		mostro.atacar(heroi);
		
		System.out.println(mostro.vida);
		System.out.println(heroi.vida);
		
			
		
